{
  "Version": "2012-10-17",
  "Statement": [
    {
			"Effect": "Allow",
			"Action": [
				"s3:*",
				"s3-object-lambda:*"
			],
			"Resource": [
				"*"
			]
		},
		{
			"Effect": "Allow",
			"Action": "cloudfront:*",
			"Resource": [
				"*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"logs:*",
				"apigateway:*"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": "dynamodb:*",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"ecr:*",
				"ecr-public:GetAuthorizationToken"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"iam:get*",
				"iam:put*",
				"iam:list*",
				"iam:createrole",
				"iam:attach*",
				"iam:TagRole"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": "iam:passRole",
			"Resource": [
				"arn:aws:iam::<account-id>:role/chicago-crimes-lambda-execution-role"
			]
		},
		{
			"Effect": "Allow",
			"Action": "lambda:*",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": "ses:*",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"application-autoscaling:*",
				"sts:*"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"cloudwatch:*"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"logs:*"
			],
			"Resource": "*"
		}
  ]
}
